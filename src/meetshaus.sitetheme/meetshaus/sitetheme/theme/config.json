{
    "name": "meetshaus",
    "paths": {
        "app": "app/",
        "base": "../",
        "dev": "_site/",
        "dist": "dist/",
        "diazoPrefix": "/++theme++meetshaus.sitetheme/",
        "src": "node_modules/"
    },
    "favicon": {
        "dataFile": "faviconData.json",
        "masterPicture": "meetshaus-favicon.png",
        "iconPath": "assets/ico/",
        "ios": "#ffffff",
        "windows": "#2d89ef",
        "androidBackground": "#ffffff",
        "androidColor": "#ffffff",
        "safariColor": "#5bbad5",
        "revisionKey": "iet7OjeChoz2",
        "html": "_includes/base/head.html",
        "htmlDist": "_includes/base/"
    },
    "scripts": {
        "src": [
            "JVFloat/jvfloat.js",
            "flickity/dist/flickity.pkgd.js",
            "fontfaceobserver/fontfaceobserver.js",
            "hideShowPassword/hideShowPassword.js",
            "lazysizes/lazysizes-umd.js",
            "lazysizes/plugins/parent-fit/ls.parent-fit.js",
            "requirejs/requires.js",
            "respimage/respimage.js"

        ],
        "app": [
            "scripts/a25.js",
            "scripts/a25.helpers.js",
            "scripts/main.js"
        ],
        "base": [
            "jquery/dist/jquery.js",
            "modernizr/modernizr.js",
            "tether/dist/js/tether.min.js",
            "bootstrap/dist/js/bootstrap.js",
            "fontfaceobserver/fontfaceobserver.js",
            "mailcheck/src/mailcheck.js",
            "JVFloat/jvfloat.js",
            "hideShowPassword/hideShowPassword.js",
            "lazysizes/plugins/ls.parent-fit.js",
            "lazysizes/lazysizes.js",
            "respimage/respimage.js",
            "flickity/dist/flickity.pkgd.js"
        ]
    },
    "styles": {
        "src": [
            "rfs/scss/_rfs.scss",
            "sass-mq/_mq.scss"
        ]
    },
    "replacementPatterns": {
        "base": [
            {
                "match": "/../dist/styles/",
                "replacement": "/styles/"
            },
            {
                "match": "/dist/styles/",
                "replacement": "/styles/"
            }
        ],
        "server": [
            {
                "match": "../../assets/",
                "replacement": "../assets/"
            },
            {
                "match": "../../dist/styles/",
                "replacement": "/styles/"
            },
            {
                "match": "../dist/styles/",
                "replacement": "/styles/"
            },
            {
                "match": "../../dist/scripts/",
                "replacement": "/scripts/"
            },
            {
                "match": "../dist/scripts/",
                "replacement": "/scripts/"
            }
        ],
        "pat": [
            {
                "match": "../assets/",
                "replacement": "assets/"
            },
            {
                "match": "assets/",
                "replacement": "/++theme++meetshaus.sitetheme/dist/assets/"
            },
            {
                "match": "../styles/",
                "replacement": "/++theme++meetshaus.sitetheme/dist/styles/"
            },
            {
                "match": "styles/",
                "replacement": "/++theme++meetshaus.sitetheme/dist/styles/"
            },
            {
                "match": "/styles/",
                "replacement": "/++theme++meetshaus.sitetheme/dist/styles/"
            },
            {
                "match": "../scripts/",
                "replacement": "/++theme++meetshaus.sitetheme/scripts/"
            },
            {
                "match": "scripts/",
                "replacement": "/++theme++meetshaus.sitetheme/scripts/"
            }
        ]
    }
}
